{
  "rules": {
    "$wuct": {
      ".read": "auth != null && auth.token.email == 'wuct@clive.io'",
      ".write": "($wuct == 'wuct2022' || $wuct == 'wucttest') && auth != null && auth.token.email == 'wuct@clive.io'",
      "answers": {
        "$id": {
          "$passcode": {
            ".read": "true",
            "currPacket": {
              ".write": "($wuct == 'wuct2022' || $wuct == 'wucttest') && data.parent().exists() && data.parent().parent().parent().parent().child('timer').child('target').val() >= now - 10000 && data.parent().parent().parent().parent().child('timer').child('target').val() - data.parent().parent().parent().parent().child('timer').child('timerlength').val() * 60000 <= now + 10000",
            },
            "$d": {
              "$n": {
                ".write": "($wuct == 'wuct2022' || $wuct == 'wucttest') && data.parent().parent().exists() && !data.child('submit').exists() && data.parent().parent().parent().parent().parent().child('timer').child('target').val() >= now - 10000 && data.parent().parent().parent().parent().parent().child('timer').child('target').val() - data.parent().parent().parent().parent().parent().child('timer').child('timerlength').val() * 60000 <= now + 10000"
              }
            }
          }
        }
      },
      "questions": {
        ".read": "($wuct == 'wuct2022' || $wuct == 'wucttest') && data.parent().child('timer').child('target').val() >= now - 10000 && data.parent().child('timer').child('target').val() - data.parent().child('timer').child('timerlength').val() * 60000 <= now + 10000"
      },
      "scores": { ".read": "$wuct != 'wuct2022' || ($wuct == 'wuct2022' && data.parent().child('timer').child('target').val() >= now - 30000)" },
      "teams": { ".read": "true" },
      "timer": { ".read": "true" },
    }
  }
}
